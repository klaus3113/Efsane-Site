<%- include('menu/admin-header', {bot, user, path}) %>

<% if (bot.admin.includes(user.id) !== true) { %>
  <meta http-equiv = "refresh" content = "0; url = /yetkili/hata" />
<% } %>

    <main><div class="container">
  <div class="row equal-height-grid">

    <div class="col l9 s12">

      <div class="card">
        <div class="card-content">
          <span class="card-title"></span>
          <p><%=bot.user.username%> - Admin Panel</p>
        </div>

        <div class="divider"></div>

        <div class="card-content">
          <div class="chat-wrapper">

            <div class="chat-message">
              <img class="circle" src="https://cdn.discordapp.com/avatars/<%=user.id%>/<%=user.avatar%>.png" alt="avatar">
              Hoşgeldin patron, admin panelde yeni moderatör ekleyebilirsin veya mevcut bir moderatörü silebilirsin.
            </div>
        </div>
        </div>

    </div>

   

    </main>
